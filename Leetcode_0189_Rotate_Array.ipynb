    "def rotate(nums, k):\n",
    "    n = len(nums)\n",
    "    \n",
    "    k = k%n\n",
    "    \n",
    "    nums[:k],nums[k:] = nums[n-k:],nums[:n-k]\n",
    "    print(nums)\n",
    "\n",
    "nums = [1,2,3,4,5,6,7]\n",
    "k = 3\n",
    "# nums = [-1,-100,3,99]\n",
    "# k = 2\n",
    "result = rotate(nums,k)"
   ]
  }
