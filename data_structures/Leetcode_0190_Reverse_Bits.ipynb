   "source": [
    "def reverseBits(n):\n",
    "    res = 0\n",
    "    for i in range(32):\n",
    "        bit = (n >> i) & 1\n",
    "        res += (bit << (31 - i))\n",
    "    return res\n",
    "\n",
    "n = 0b00000010100101000001111010011100\n",
    "res = reverseBits(n)\n",
    "print(res)"
   ]
  }
