   "source": [
    "def climbStairs(nums):\n",
    "    rob1, rob2 = 0, 0\n",
    "\n",
    "    for n in nums:\n",
    "        temp = max(n + rob1, rob2)\n",
    "        rob1 = rob2\n",
    "        rob2 = temp\n",
    "    return rob2\n",
    "\n",
    "n = [1,2,3,1]\n",
    "res = climbStairs(n)\n",
    "print(res)"
   ]
  }
