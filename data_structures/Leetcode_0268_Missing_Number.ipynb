    "def missingNumber(nums):\n",
    "    res = len(nums)\n",
    "\n",
    "    for i in range(len(nums)):\n",
    "        res += i - nums[i]\n",
    "    return res\n",
    "print(missingNumber(nums = [3,0,1]))"
