   "source": [
    "## Method1 - Two Pointers - Reverse String"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "bacdfe\n"
     ]
    }
   ],
   "source": [
    "def reverseStr(s, k):\n",
    "    n = len(s)\n",
    "    res = [c for c in s]\n",
    "    for l in range(0, n, 2*k):\n",
    "    \n",
    "        r = (l + k )\n",
    "        res[l:r] = res[l:r][::-1]\n",
    "\n",
    "    return \"\".join(res)\n",
    "\n",
    "s = \"abcdef\"\n",
    "k = 2\n",
    "result = reverseStr(s, k)\n",
    "print(result)"
   ]
  }
 ],
